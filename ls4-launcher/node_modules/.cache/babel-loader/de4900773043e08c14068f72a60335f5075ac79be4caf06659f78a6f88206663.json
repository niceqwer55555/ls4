{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  props: {\n    custom: Boolean,\n    summoner: Object\n  },\n  mounted() {\n    console.log(this.summoner);\n  },\n  methods: {\n    getSummonerIconUrl() {\n      const {\n        host,\n        port\n      } = this.$store.state.config.download;\n      return `${host}:${port}/summoner_icons/${this.summoner.summonerIconId}.png`;\n    },\n    isCaptain() {\n      return this.summoner.summonerName == this.$store.state.lobby.owner.summonerName;\n    },\n    canKick() {\n      return this.$store.state.lobbyOwner && this.summoner.summonerName != this.$store.state.user.summonerName;\n    },\n    isFriend() {\n      if (this.summoner.summonerName == this.$store.state.user.summonerName) {\n        return true;\n      }\n      return this.$store.state.friendList.filter(friend => {\n        return friend.summonerName == this.summoner.summonerName;\n      }).length > 0;\n    },\n    addFriend() {\n      this.$socket.sendSystemMessage(\"FRIEND_OUT\", {\n        data: this.summoner.summonerName\n      }, (response, error) => {\n        if (error) {\n          console.log(\"Flyback error:\");\n          console.log(error);\n        }\n      });\n    },\n    kickUser() {\n      this.$socket.sendLobbyMessage(\"LOBBY_KICK\", {\n        data: this.summoner.summonerName\n      }, (response, error) => {\n        if (error) {\n          console.log(\"Flyback error:\");\n          console.log(error);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["props","custom","Boolean","summoner","Object","mounted","console","log","methods","getSummonerIconUrl","host","port","$store","state","config","download","summonerIconId","isCaptain","summonerName","lobby","owner","canKick","lobbyOwner","user","isFriend","friendList","filter","friend","length","addFriend","$socket","sendSystemMessage","data","response","error","kickUser","sendLobbyMessage"],"sources":["src/components/Lobby/Player.vue"],"sourcesContent":["<template>\r\n  <div class=\"player\" :class=\"{ custom: custom }\">\r\n    <div class=\"icon\">\r\n      <img :src=\"getSummonerIconUrl()\" alt=\"summonerIcon\" />\r\n    </div>\r\n    <div class=\"details\">\r\n      <div\r\n        class=\"name\"\r\n        :class=\"{ shop_colored_name: summoner.nameColourUnlocked }\"\r\n      >\r\n        {{ summoner.summonerName }}\r\n      </div>\r\n      <div class=\"level\">Level {{ summoner.summonerLevel }}</div>\r\n      <div class=\"badges\">\r\n        <div class=\"badge\" v-if=\"isCaptain()\">\r\n          <img src=\"@/assets/images/captain_logo.png\" alt=\"captain\" />\r\n        </div>\r\n        <div class=\"badge disabled btn\">\r\n          <img\r\n            src=\"@/assets/images/lobby_small_btn.png\"\r\n            alt=\"lobbyProfileBtn\"\r\n          />\r\n          <i class=\"fas fa-user\"></i>\r\n        </div>\r\n        <div class=\"badge btn\" v-if=\"!isFriend()\">\r\n          <img\r\n            src=\"@/assets/images/lobby_adduser_btn.png\"\r\n            alt=\"addFriendBtn\"\r\n            @click=\"addFriend\"\r\n          />\r\n        </div>\r\n        <div class=\"badge btn\" v-if=\"canKick()\">\r\n          <img\r\n            src=\"@/assets/images/lobby_small_btn.png\"\r\n            alt=\"kickUserBtn\"\r\n            @click=\"kickUser\"\r\n          />\r\n          <i class=\"fas fa-times\"></i>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: {\r\n    custom: Boolean,\r\n    summoner: Object\r\n  },\r\n  mounted() {\r\n    console.log(this.summoner);\r\n  },\r\n  methods: {\r\n    getSummonerIconUrl() {\r\n      const { host, port } = this.$store.state.config.download;\r\n      return `${host}:${port}/summoner_icons/${this.summoner.summonerIconId}.png`;\r\n    },\r\n    isCaptain() {\r\n      return (\r\n        this.summoner.summonerName == this.$store.state.lobby.owner.summonerName\r\n      );\r\n    },\r\n    canKick() {\r\n      return (\r\n        this.$store.state.lobbyOwner &&\r\n        this.summoner.summonerName != this.$store.state.user.summonerName\r\n      );\r\n    },\r\n    isFriend() {\r\n      if (this.summoner.summonerName == this.$store.state.user.summonerName) {\r\n        return true;\r\n      }\r\n      return (\r\n        this.$store.state.friendList.filter(friend => {\r\n          return friend.summonerName == this.summoner.summonerName;\r\n        }).length > 0\r\n      );\r\n    },\r\n    addFriend() {\r\n      this.$socket.sendSystemMessage(\r\n        \"FRIEND_OUT\",\r\n        { data: this.summoner.summonerName },\r\n        (response, error) => {\r\n          if (error) {\r\n            console.log(\"Flyback error:\");\r\n            console.log(error);\r\n          }\r\n        }\r\n      );\r\n    },\r\n    kickUser() {\r\n      this.$socket.sendLobbyMessage(\r\n        \"LOBBY_KICK\",\r\n        { data: this.summoner.summonerName },\r\n        (response, error) => {\r\n          if (error) {\r\n            console.log(\"Flyback error:\");\r\n            console.log(error);\r\n          }\r\n        }\r\n      );\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.player {\r\n  width: 100%;\r\n  height: 17%;\r\n  background-blend-mode: multiply;\r\n  border: 1.5px solid #474b4c;\r\n  border-radius: 4px;\r\n  background: url(\"../../assets/images/noise-mask.png\"),\r\n    linear-gradient(180deg, #363636 0%, #090b0a 100%);\r\n  display: flex;\r\n  margin-bottom: 5px;\r\n  padding: 4px;\r\n}\r\n\r\n.player .icon {\r\n  margin-right: 10px;\r\n  width: 55px;\r\n  height: 55px;\r\n}\r\n\r\n.player.custom .icon {\r\n  width: 50px;\r\n  height: 50px;\r\n}\r\n\r\n.player .icon img {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.player .details {\r\n  display: flex;\r\n  height: 100%;\r\n  width: 200px;\r\n  flex-direction: column;\r\n}\r\n\r\n.player .details .name {\r\n  height: 35%;\r\n  font-size: 14px;\r\n  font-family: LoLFont2;\r\n}\r\n\r\n.player .details .level {\r\n  height: 23%;\r\n  font-size: 10px;\r\n  font-family: LoLFont2;\r\n}\r\n\r\n.player .details .badges {\r\n  height: 42%;\r\n  padding: 2px 0;\r\n  display: flex;\r\n}\r\n\r\n.player .details .badges .badge {\r\n  margin-right: 10px;\r\n  height: 100%;\r\n  width: 15px;\r\n  position: relative;\r\n}\r\n\r\n.player .details .badges .badge.btn {\r\n  transition: filter ease-in-out 200ms;\r\n  cursor: pointer;\r\n}\r\n\r\n.player .details .badges .badge.btn:not(.disabled):hover {\r\n  filter: brightness(1.25);\r\n}\r\n\r\n.player .details .badges .badge.disabled {\r\n  filter: grayscale(100%);\r\n}\r\n\r\n.player .details .badges .badge i {\r\n  font-size: 10px;\r\n  position: absolute;\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n.player .details .badges .badge img {\r\n  position: absolute;\r\n  left: 50%;\r\n  top: 50%;\r\n  transform: translate(-50%, -50%);\r\n  height: 100%;\r\n  width: auto;\r\n  object-fit: contain;\r\n  object-position: center;\r\n}\r\n</style>\r\n"],"mappings":";;AA6CA;EACAA,KAAA;IACAC,MAAA,EAAAC,OAAA;IACAC,QAAA,EAAAC;EACA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA,MAAAJ,QAAA;EACA;EACAK,OAAA;IACAC,mBAAA;MACA;QAAAC,IAAA;QAAAC;MAAA,SAAAC,MAAA,CAAAC,KAAA,CAAAC,MAAA,CAAAC,QAAA;MACA,UAAAL,IAAA,IAAAC,IAAA,wBAAAR,QAAA,CAAAa,cAAA;IACA;IACAC,UAAA;MACA,OACA,KAAAd,QAAA,CAAAe,YAAA,SAAAN,MAAA,CAAAC,KAAA,CAAAM,KAAA,CAAAC,KAAA,CAAAF,YAAA;IAEA;IACAG,QAAA;MACA,OACA,KAAAT,MAAA,CAAAC,KAAA,CAAAS,UAAA,IACA,KAAAnB,QAAA,CAAAe,YAAA,SAAAN,MAAA,CAAAC,KAAA,CAAAU,IAAA,CAAAL,YAAA;IAEA;IACAM,SAAA;MACA,SAAArB,QAAA,CAAAe,YAAA,SAAAN,MAAA,CAAAC,KAAA,CAAAU,IAAA,CAAAL,YAAA;QACA;MACA;MACA,OACA,KAAAN,MAAA,CAAAC,KAAA,CAAAY,UAAA,CAAAC,MAAA,CAAAC,MAAA;QACA,OAAAA,MAAA,CAAAT,YAAA,SAAAf,QAAA,CAAAe,YAAA;MACA,GAAAU,MAAA;IAEA;IACAC,UAAA;MACA,KAAAC,OAAA,CAAAC,iBAAA,CACA,cACA;QAAAC,IAAA,OAAA7B,QAAA,CAAAe;MAAA,GACA,CAAAe,QAAA,EAAAC,KAAA;QACA,IAAAA,KAAA;UA